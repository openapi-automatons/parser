{
  "name": "@automatons/parser",
  "version": "0.0.50",
  "repository": "git@github.com:openapi-automatons/parser.git",
  "author": "tanmen <yt.prog@gmail.com>",
  "license": "MIT",
  "keywords": [
    "openapi",
    "openapi-automatons",
    "automatons"
  ],
  "main": "index.js",
  "scripts": {
    "prebuild": "depcheck && node scripts/prebuild.js",
    "build": "tsc && cp package.json README.md LICENSE dist",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "postinstall": "husky install && typesync && yarn check --integrity || yarn install --ignore-scripts && yarn-deduplicate",
    "postversion": "cp package.json ../package.json"
  },
  "dependencies": {
    "@automatons/tools": "^1.0.52",
    "change-case": "^4.1.1",
    "deep-equal": "^2.0.3",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^8.0.3",
    "@types/deep-equal": "^1.0.1",
    "@types/eslint": "^8.2.2",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.9",
    "@types/semantic-release": "^17.2.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^7.0.4",
    "jest": "^26.0.1",
    "lint-staged": "^12.1.7",
    "semantic-release": "^18.0.1",
    "ts-jest": "^26.0.0",
    "typescript": "^4.5.4",
    "typesync": "^0.8.0",
    "yarn-deduplicate": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
